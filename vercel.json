{
	"version": 2,
	"builds": [
		{
			"src": "django_app_name/wsgi.py",
			"use": "@vercel/python"
		},
		{
			"src": "build_files.sh",
			"use": "@vercel/static-build",
			"config": {
				"distDir": "staticfiles"
			}
		}
	],
	"routes": [
		{
			"src": "/static/(.*)",
			"dest": "/static/$1"
		},
		{
			"src": "/(.*)",
			"dest": "onlinequiz/wsgi.py"
		}
	],
	"outputDirectory": "staticfiles"
}