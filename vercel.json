{
	"version": 2,
	"builds": [
		{
			"src": "onlinequiz/wsgi.py",
			"use": "@vercel/python",
			"config": {
				"maxLambdaSize": "15mb",
				"runtime": "python3.7"
			}
		},
		{
			"src": "build_files.sh",
			"use": "@vercel/static",
			"config": {
				"distDir": "staticfiles"
			}
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "onlinequiz/wsgi.py"
		}
	]
}